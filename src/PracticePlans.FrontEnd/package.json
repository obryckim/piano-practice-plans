{
  "name": "practiceplans.frontend",
  "version": "1.0.0",
  "description": "Web front end for the piano practice plans solution",
  "author": "M.Obrycki",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "esw --format simple-detail webpack.config.* \"src/**/*.jsx\" tools",
    "lint:watch": "npm run lint -s -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\" \"src/**/*.test.jsx\"",
    "test:watch": "npm run test -- --watch --watch-extensions js,jsx",
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obryckim/piano-practice-plans.git"
  },
  "bugs": {
    "url": "https://github.com/obryckim/piano-practice-plans/issues"
  },
  "homepage": "https://github.com/obryckim/piano-practice-plans#readme",
  "dependencies": {
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "colors": "^1.3.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "expect": "^23.6.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^12.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "opn": "^5.4.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0"
  }
}
